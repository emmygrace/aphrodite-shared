{
  "name": "@gaia-tools/aphrodite-shared",
  "version": "0.1.0",
  "description": "Platform-agnostic wheel definitions, visual/glyph configs, and chart presets for Aphrodite",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./wheels": {
      "import": "./dist/wheels/index.mjs",
      "require": "./dist/wheels/index.js",
      "types": "./dist/wheels/index.d.ts"
    },
    "./configs": {
      "import": "./dist/configs/index.mjs",
      "require": "./dist/configs/index.js",
      "types": "./dist/configs/index.d.ts"
    },
    "./presets": {
      "import": "./dist/presets/index.mjs",
      "require": "./dist/presets/index.js",
      "types": "./dist/presets/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/wheels/index.ts src/configs/index.ts src/presets/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "test": "echo \"No tests yet\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "astrology",
    "chart",
    "wheel",
    "preset",
    "typescript"
  ],
  "author": "Emmy Grace",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:emmygrace/aphrodite-core.git"
  },
  "homepage": "https://github.com/emmygrace/aphrodite-core",
  "bugs": {
    "url": "https://github.com/emmygrace/aphrodite-core/issues"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "tsup": "^8.0.0",
    "typescript": "^5.6.3"
  }
}

